    "edition": "Enterprise",
    "features": [
        "LightningSalesConsole",
        "HighVelocitySales",
        "CPQ",
        "Communities",
        "ContactsToMultipleAccounts",
        "StateAndCountryPicklist",
        "MultiCurrency",
        "PlatformCache",
        "CustomNotificationType",
        "DebugApex",
        "EnableSetPasswordInApi",
        "SalesCloudEinstein"
